<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.lab1024.sa.admin.module.business.stock.dao.StockInfoDao">

    <!-- 分页查询 -->
    <select id="queryPage" resultType="net.lab1024.sa.admin.module.business.stock.domain.vo.StockInfoVO">
        SELECT
        *
        FROM t_stock_info
        <where>
            <!--商品编码/名称-->
            <if test="queryForm.goodsId != null and queryForm.goodsId != ''">
                        AND INSTR(t_stock_info.goods_id,#{queryForm.goodsId})
            </if>
            <!--仓库编码/名称-->
            <if test="queryForm.depotId != null and queryForm.depotId != ''">
                        AND INSTR(t_stock_info.depot_id,#{queryForm.depotId})
            </if>
        </where>
    </select>


</mapper>